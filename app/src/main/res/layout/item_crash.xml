<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/y120"
    android:background="@drawable/selector_press_effect">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/y120"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/item_crash_model_tv"
            android:layout_width="@dimen/x380"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/x40"
            android:gravity="center"
            android:maxLines="1"
            android:textColor="@color/colorTextDark"
            android:textSize="@dimen/textSizeNormal"/>


        <TextView
            android:id="@+id/item_crash_api_tv"
            android:layout_width="@dimen/x90"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/x40"
            android:gravity="center"
            android:maxLines="1"
            android:textColor="@color/colorTextDark"
            android:textSize="@dimen/textSizeSmall"/>

        <TextView
            android:id="@+id/item_crash_version_tv"
            android:layout_width="@dimen/x130"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/x40"
            android:gravity="center"
            android:maxLines="1"
            android:textColor="@color/colorTextDark"
            android:textSize="@dimen/textSizeSmall"/>

        <TextView
            android:id="@+id/item_crash_time_tv"
            android:layout_width="@dimen/x300"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/x40"
            android:gravity="center"
            android:maxLines="1"
            android:textColor="@color/colorTextDark"
            android:textSize="@dimen/textSizeTiny"/>

    </LinearLayout>

    <include layout="@layout/layout_common_line_bottom"/>
</RelativeLayout>